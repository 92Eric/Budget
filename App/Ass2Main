eric.zoo;import android.app.AlertDialog;import android.content.DialogInterface;import android.content.Intent;import android.net.Uri;import android.support.v7.app.AppCompatActivity;import android.os.Bundle;import android.util.Log;import android.view.Menu;import android.view.MenuItem;import android.view.View;import android.widget.AdapterView;import android.widget.ListView;import java.util.ArrayList;import java.util.List;public class ListViewZoo extends AppCompatActivity {    @Override    protected void onCreate(Bundle savedInstanceState) {        super.onCreate(savedInstanceState);        setContentView(R.layout.custom_list_view);      //  setContentView(R.layout.menu);        ListView listView = (ListView) findViewById(R.id.custom_view);        List<Zoo> zoo = new ArrayList<>();        zoo.add(new Zoo("Zebra", "zebrapic.jpg"));        zoo.add(new Zoo("Lion", "lion.jpg"));        zoo.add(new Zoo("Bear", "bear.jpg"));        zoo.add(new Zoo("Rabbit", "rabbit.jpg"));        zoo.add(new Zoo("Monkey", "monkey.jpg"));        listView.setAdapter(new CustomAdapter(this, R.layout.activity_list_view_zoo, zoo));        listView.setOnItemClickListener(new AdapterView.OnItemClickListener() {            @Override            public void onItemClick(AdapterView<?> parent, View view, int position, long id) {                 Log.d("CECS 345","clicked: " + position);                if(position == 0)                {                     Intent pass = new Intent(ListViewZoo.this, animal_detail.class);                     Bundle bundle1 = new Bundle();                     bundle1.putString("name", "Zebra");                     pass.putExtras(bundle1);                     startActivityForResult(pass,1111);                } else if(position == 1)                  {                      Intent pass = new Intent(ListViewZoo.this, animal_detail.class);                      Bundle bundle1 = new Bundle();                      bundle1.putString("name", "Lion");                      pass.putExtras(bundle1);                      startActivityForResult(pass,1111);                  } else if(position == 2)                    {                        Intent pass = new Intent(ListViewZoo.this, ZooName.class);                        Bundle bundle1 = new Bundle();                        bundle1.putString("name", "Bear");                        pass.putExtra
